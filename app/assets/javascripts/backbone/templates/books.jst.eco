<div id="books-wrap" class="span8 offset2">
  <h2>Books</h2>
  <div class="well">
  <% if @books: %>
    <ul id="book-list" class="unstyled">
      <% for book in @books.models: %>        
        <li id="book-id<%= book.get('id') %>" class="book-listing">
          <%= book.get('name') %>
        </li>      
      <% end %>
    </ul>
  <% end %>
  </div>
</div>
