<div id="books-wrap" class="span8 offset2">
  <h2>Books</h2>
  <div class="well">
  <% if @books: %>
    <ul id="book_list" class="unstyled">
      <% for book in @books.pluck("name"): %>
      <li class="book_listing">
        <%= book %>
      </li>      
      <% end %>
    </ul>
  <% end %>
  </div>
</div>
